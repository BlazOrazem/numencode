{
    "name": "numencode/cms",
    "type": "project",
    "description": "Numencode - Content Management System",
    "keywords": ["numencode", "laravel"],
    "homepage": "https://github.com/BlazOrazem/numencode",
    "license": "MIT",
    "authors": [
      {
        "name": "Blaz Orazem",
        "email": "blaz@orazem.si",
        "homepage": "http://www.orazem.si",
        "role": "Developer"
      }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "^6.2",
        "intervention/image": "^2.3",
        "intervention/imagecache": "^2.3",
        "laracasts/utilities": "^2.1",
        "laraplus/translatable": "^1.0",
        "laravel/framework": "5.4.*",
        "laravel/socialite": "~3.0",
        "laravel/tinker": "~1.0",
        "rap2hpoutre/laravel-log-viewer": "^0.8.0",
        "spatie/laravel-analytics": "^1.4",
        "unisharp/laravel-filemanager": "^1.7"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "~2.0@dev",
        "barryvdh/laravel-ide-helper": "^2.1",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/Utils/helpers.php",
            "app/Utils/debug.php"
        ],
        "psr-4": {
            "Numencode\\": "app/",
            "Admin\\": "modules/Admin/",
            "Cms\\": "modules/Cms/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
